echo "$(date +%Y-%m-%d\ %H:%M) Cron Started" >> /dev/console
curl -sX POST -H 'Content-type: application/json' --data '{"text":"Cron Started\n"}' $SLACK_HOOK >> /dev/stdout 2>&1

curl https://jjosh.de/wp-cron.php >> /dev/console

echo "$(date +%Y-%m-%d\ %H:%M) Cron Finished" >> /dev/console
curl -sX POST -H 'Content-type: application/json' --data '{"text":"Cron Finished\n"}' $SLACK_HOOK >> /dev/stdout 2>&1
